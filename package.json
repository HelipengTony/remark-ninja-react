{
  "name": "remark-ninja-react",
  "version": "2.3.8",
  "description": "React component for Remark Ninja",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "build": "nwb build-react-component && cp src/*.css lib/",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "dependencies": {
    "axios": "^0.19.0"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  },
  "devDependencies": {
    "nwb": "0.23.x",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "author": "Remark Ninja <support@remark.ninja>",
  "homepage": "https://remark.ninja",
  "license": "MIT",
  "repository": "github:remark-ninja/remark-ninja-react",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "comments",
    "react-component"
  ]
}
